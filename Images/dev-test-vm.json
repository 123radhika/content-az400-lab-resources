{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
  },
  "variables": {
    "labSubnetName": "[concat(variables('labVirtualNetworkName'), 'Subnet')]",
    "labVirtualNetworkId": "[resourceId('Microsoft.DevTestLab/labs/virtualnetworks', variables('existingLabName'), variables('labVirtualNetworkName'))]",
    "labVirtualNetworkName": "[concat('Dtl', variables('existingLabName'))]",
    "resourceName": "[concat(variables('existingLabName'), '/', variables('newVMName'))]",
    "resourceType": "Microsoft.DevTestLab/labs/virtualMachines",
    "userName": "cloud_user",
    "password": "LA_ACG_Forever2020!!",
    "newVMSize": "Standard_B2s",
    "version": "latest",
    "publisher": "MicrosoftWindowsServer",
    "sku": "2012-R2-Datacenter",
    "osType": "Windows",
    "offer": "WindowsServer",
    "newVMName": "labWinVM-028120",
    "existingLabName": "devtest-lab28"
  },
  "resources": [
    {
      "apiVersion": "2018-10-15-preview",
      "type": "Microsoft.DevTestLab/labs/virtualMachines",
      "name": "[variables('resourceName')]",
      "location": "[resourceGroup().location]",
      "properties": {
        "size": "[variables('newVMSize')]",
