          "OsType": "[variables('osType')]",
          "Publisher": "[variables('publisher')]",
          "Offer": "[variables('offer')]",
          "Version": "[variables('version')]"
        }
      }
    }
  ],
  "outputs": {
    "vmId": {
      "type": "string",
      "value": "[resourceId(variables('resourceType'), variables('existingLabName'), variables('newVMName'))]"
    }
  }
